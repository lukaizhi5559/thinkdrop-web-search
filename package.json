{
  "name": "thinkdrop-web-search-service",
  "version": "1.0.0",
  "description": "Web search microservice with MCP protocol support for ThinkDrop AI",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "NODE_ENV=test jest --coverage",
    "test:integration": "NODE_ENV=test jest --testPathPattern=integration",
    "test:load": "node tests/load/load-test.js",
    "test:search": "node test-search.js",
    "db:init": "node src/database/init.js",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "web-search",
    "newsapi",
    "duckduckgo",
    "mcp",
    "microservice",
    "thinkdrop"
  ],
  "author": "ThinkDrop AI Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.0",
    "axios": "^1.6.0",
    "duckdb": "^0.9.0",
    "uuid": "^9.0.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.0.0",
    "node-cache": "^5.1.0"
  },
  "devDependencies": {
    "jest": "^29.0.0",
    "supertest": "^6.3.0",
    "nodemon": "^3.0.0",
    "nock": "^13.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
